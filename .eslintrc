{
    "globals": {
        "jQuery": true,
        "before": true,
        "process": true
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true,
            "modules": true
        }
    },
    "env": {
        "browser": true,
        "es6": true,
        "node": true,
        "jest": true
    },
    "parser": "babel-eslint",
    "plugins": [
        "react"
    ],
    "extends": ["eslint:recommended", "plugin:react/all"],
    "rules": {
        "react/forbid-prop-types": 0,
        "react/jsx-boolean-value": [1, "always"],
        "react/jsx-first-prop-new-line": [1, "multiline"],
        "react/jsx-max-props-per-line": 0,
        "react/jsx-no-bind": 0,
        "react/jsx-no-literals": 0,
        "react/jsx-sort-props": 0,
        "react/no-danger": 0,
        "react/no-deprecated": 0,
        "react/no-direct-mutation-state": 0,
        "react/no-multi-comp": [1, { "ignoreStateless": true }],
        "react/no-set-state": 0,
        "react/require-optimization": 0,
        "react/sort-prop-types": 0,
        "react/jsx-closing-bracket-location": [1, "line-aligned"],

        "no-alert": 1,
        "no-array-constructor": 1,
        "no-bitwise": 1,
        "no-caller": 1,
        "no-catch-shadow": 1,
        "no-class-assign": 1,
        "no-cond-assign": 2,
        "no-console": 0,
        "no-const-assign": 1,
        "no-constant-condition": 2,
        "no-continue": 0,
        "no-control-regex": 2,
        "no-debugger": 2,
        "no-delete-var": 2,
        "no-div-regex": 1,
        "no-dupe-class-members": 1,
        "no-dupe-keys": 2,
        "no-dupe-args": 2,
        "no-duplicate-case": 2,
        "no-else-return": 0,
        "no-empty": 2,
        "no-empty-character-class": 2,
        "no-eq-null": 1,
        "no-eval": 1,
        "no-ex-assign": 2,
        "no-extend-native": 1,
        "no-extra-bind": 1,
        "no-extra-boolean-cast": 2,
        "no-extra-parens": 0,
        "no-extra-semi": 2,
        "no-fallthrough": 2,
        "no-floating-decimal": 1,
        "no-func-assign": 2,
        "no-implicit-coercion": 0,
        "no-implied-eval": 1,
        "no-inline-comments": 0,
        "no-inner-declarations": [
            2,
            "functions"
        ],
        "no-invalid-regexp": 2,
        "no-invalid-this": 0,
        "no-irregular-whitespace": 2,
        "no-iterator": 1,
        "no-labels": 1,
        "no-lone-blocks": 1,
        "no-lonely-if": 0,
        "no-loop-func": 1,
        "no-mixed-requires": [
            1,
            false
        ],
        "no-mixed-spaces-and-tabs": [
            2,
            false
        ],
        "linebreak-style": [
            1,
            "unix"
        ],
        "no-multi-spaces": 1,
        "no-multi-str": 1,
        "no-multiple-empty-lines": [
            "error",
            {
                "max": 2,
                "maxEOF": 1,
                "maxBOF": 1
            }
        ],
        "no-native-reassign": 1,
        "no-negated-condition": 0,
        "no-negated-in-lhs": 2,
        "no-nested-ternary": 1,
        "no-new": 1,
        "no-new-func": 1,
        "no-new-object": 1,
        "no-new-require": 1,
        "no-new-wrappers": 1,
        "no-obj-calls": 2,
        "no-octal": 2,
        "no-octal-escape": 1,
        "no-param-reassign": 0,
        "no-path-concat": 1,
        "no-plusplus": 0,
        "no-process-env": 0,
        "no-process-exit": 1,
        "no-proto": 1,
        "no-redeclare": 2,
        "no-regex-spaces": 2,
        "no-restricted-modules": 1,
        "no-restricted-syntax": 1,
        "no-return-assign": 0,
        "no-script-url": 1,
        "no-self-compare": 1,
        "no-sequences": 1,
        "no-shadow": 1,
        "no-shadow-restricted-names": 1,
        "no-spaced-func": 1,
        "no-sparse-arrays": 2,
        "no-sync": 1,
        "no-ternary": 0,
        "no-trailing-spaces": 1,
        "no-this-before-super": 1,
        "no-throw-literal": 1,
        "no-undef": 2,
        "no-undef-init": 1,
        "no-undefined": 0,
        "no-unexpected-multiline": 1,
        "no-underscore-dangle": [1, {"enforceInMethodNames": true }],
        "no-unneeded-ternary": 1,
        "no-unreachable": 2,
        "no-unused-expressions": 1,
        "no-unused-vars": [
            2,
            {
                "vars": "all",
                "args": "after-used"
            }
        ],
        "no-use-before-define": 0,
        "no-useless-call": 0,
        "no-useless-concat": 1,
        "no-void": 1,
        "no-var": 0,
        "no-warning-comments": [
            1,
            {
                "terms": [
                    "todo",
                    "fixme",
                    "xxx"
                ],
                "location": "start"
            }
        ],
        "no-with": 1,
        "array-bracket-spacing": [
            1,
            "never"
        ],
        "arrow-parens": [
            1,
            "as-needed"
        ],
        "arrow-spacing": 1,
        "accessor-pairs": 1,
        "block-scoped-var": 1,
        "block-spacing": 1,
        "brace-style": [
            1,
            "1tbs"
        ],
        "callback-return": 0,
        "camelcase": [
            1,
            {
                "properties": "never"
            }
        ],
        "comma-dangle": [
            2,
            "always-multiline"
        ],
        "comma-spacing": 1,
        "comma-style": 1,
        "complexity": 0,
        "computed-property-spacing": [
            1,
            "never"
        ],
        "consistent-return": 1,
        "consistent-this": [
            1,
            "self"
        ],
        "constructor-super": 1,
        "curly": [
            1,
            "all"
        ],
        "default-case": 1,
        "dot-location": [
            1,
            "property"
        ],
        "dot-notation": [
            1,
            {
                "allowKeywords": true
            }
        ],
        "eol-last": 1,
        "eqeqeq": 1,
        "func-names": 0,
        "func-style": 0,
        "generator-star-spacing": 1,
        "global-require": 0,
        "guard-for-in": 0,
        "handle-callback-err": 1,
        "id-length": 0,
        "indent": [
            1,
            4,
            {
                "SwitchCase": 1,
                "MemberExpression": 0
            }
        ],
        "init-declarations": 1,
        "jsx-quotes": [
            1,
            "prefer-double"
        ],
        "key-spacing": [
            1,
            {
                "beforeColon": false,
                "afterColon": true
            }
        ],
        "lines-around-comment": 1,
        "max-depth": 0,
        "max-len": 0,
        "max-nested-callbacks": [
            1,
            3
        ],
        "max-params": [
            1,
            4
        ],
        "max-statements": [
            0,
            11
        ],
        "new-cap": 0 ,
        "new-parens": 1,
        "newline-after-var": 1,
        "object-curly-spacing": [
            1,
            "always"
        ],
        "object-shorthand": 0,
        "one-var": [
            0,
            "always"
        ],
        "operator-assignment": 0,
        "operator-linebreak": [
            1,
            "before"
        ],
        "padded-blocks": [
            1,
            {
                "blocks": "never",
                "switches": "never",
                "classes": "never"
            }
        ],
        "prefer-arrow-callback": 0,
        "prefer-const": 0,
        "prefer-spread": 0,
        "prefer-reflect": 0,
        "prefer-template": 0,
        "quote-props": 0,
        "quotes": [
            1,
            "single"
        ],
        "radix": 1,
        "id-match": 1,
        "require-jsdoc": 0,
        "require-yield": 1,
        "semi": [
            1,
            "never"
        ],
        "semi-spacing": [
            1,
            {
                "before": false,
                "after": true
            }
        ],
        "sort-vars": 0,
        "space-before-blocks": [
            1,
            "always"
        ],
        "space-before-function-paren": [
            0,
            "always"
        ],
        "space-in-parens": [
            1,
            "never"
        ],
        "space-infix-ops": 1,
        "space-unary-ops": [
            1,
            {
                "words": true,
                "nonwords": false
            }
        ],
        "spaced-comment": 1,
        "strict": [
            1,
            "global"
        ],
        "use-isnan": 2,
        "valid-jsdoc": 1,
        "valid-typeof": 2,
        "vars-on-top": 0,
        "wrap-iife": 0,
        "wrap-regex": 1,
        "yoda": [
            1,
            "never"
        ]
    }
}
